{
  "name": "ordero-api-v2",
  "version": "0.0.1",
  "description": "Ordero API V2",
  "author": "Akbar Anung Yudha Saputra <akbarsaputrait@outlook.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "AWS_SDK_JS_SUPPRESS_MAINTENANCE_MODE_MESSAGE=1 jest --config ./jest.config.js",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register --files ./node_modules/typeorm/cli.js",
    "migration:generate": "wrap () { yarn typeorm migration:generate -d src/library/typeorm/datasource.typeorm.ts src/database/migrations/\"$*\" | cat; }; wrap",
    "migration:run": "yarn typeorm -d src/library/typeorm/datasource.typeorm.ts migration:run",
    "seed:create": "wrap () { yarn typeorm migration:create src/database/seeds/\"$*\" | cat; }; wrap",
    "seed:run": "yarn typeorm -d src/library/typeorm/datasource-seed.typeorm.ts migration:run",
    "entity:create": "wrap () { yarn typeorm entity:create src/database/entities/\"$*\" | cat; }; wrap",
    "subscriber:create": "wrap () { yarn typeorm subscriber:create src/database/subscribers/\"$*\" | cat; }; wrap"
  },
  "dependencies": {
    "@fastify/multipart": "^7.1.0",
    "@fastify/request-context": "^4.0.0",
    "@fastify/static": "^6.5.0",
    "@fastify/view": "^7.1.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.8",
    "@nestjs/platform-socket.io": "9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "9.2.1",
    "@sentry/node": "^7.10.0",
    "app-root-path": "^3.0.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bwip-js": "^3.0.2",
    "class-transformer": "0.3.2",
    "convert-html-to-pdf": "^1.0.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "exceljs": "^4.4.0",
    "express-useragent": "^1.0.15",
    "fastify-list-routes": "^1.0.0",
    "fastify-multer": "^2.0.3",
    "file-type": "^16.1.0",
    "handlebars": "^4.7.7",
    "handlebars-layouts": "^3.1.4",
    "html-entities": "^2.3.3",
    "json2csv": "^6.0.0-alpha.2",
    "libphonenumber-js": "^1.10.12",
    "lodash": "^4.17.21",
    "mysql-import": "^5.0.21",
    "mysql2": "^3.10.2",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.7.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pino-datadog": "^2.0.2",
    "pino-multi-stream": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.27.0",
    "simple-encryptor": "^4.0.0",
    "slug": "^8.2.3",
    "twilio": "^3.54.1",
    "typeorm": "^0.3.20",
    "ulid": "^2.3.0",
    "uuid": "^8.3.2",
    "validatorjs": "^3.22.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.0",
    "@types/jest": "28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.26.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "28.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-yml": "^1.12.2",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
